{
  apps : [
    {
      name      : "www",
      script    : "./bin/www",
      cwd: "./",           //根目录
      watch:[
            "bin",
            "common",
            "configs",
            "public",
            "routes",
            "views"
        ],
      error_file:"./logs/app-err.log",//错误输出日志
      out_file:"./logs/app-out.log",  //日志
      log_date_format:"YYYY-MM-DD HH:mm Z", //日期格式
      env: {
        COMMON_VARIABLE: "true"
      },
      env_production : {
        NODE_ENV: "production"
      },
      env_test : {
        NODE_ENV: "test"
      }
    },
  ],  
}
